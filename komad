def paranda_komavead():
    lause = input("Sisetage oma lause siia ilma komadetta: ")
    lause = lause.replace(" et ", ", et ")
    lause = lause.replace(" aga ", ", aga ")
    lause = lause.replace(" kuid ", ", kuid ")
    lause = lause.replace(" sest ", ", sest ")
    print(lause)

paranda_komavead()


